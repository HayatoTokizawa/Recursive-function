    "# 繰り返し処理でも実装\n",
    "\n",
    "def sum2(n):\n",
    "    total = 0\n",
    "    for i in range(n, 1, -1):\n",
    "        total += i\n",
    "    return total\n",
    "\n",
    "sum(100)"
   ]
